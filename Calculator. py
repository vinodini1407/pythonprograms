a=int(input ())
b=int(input ())
operator=input ()
if(operator == '+'):
    print (a+b)
elif (operator == '-'):
    print (a-b)
elif (operator == '*'):
    print (a*b)
elif (operator == '/'):
    print (a/b)
elif (operator == '%'):
    print (a%b)
elif (operator == '**'):
    print (a**b)
elif (operator == '//'):
    print (a//b)
